  </div>
  <div class="offcanvas offcanvas-end hide" tabindex="-1" id="create_project" aria-labelledby="create_project"
    aria-modal="true" role="dialog">
    <div class="offcanvas-header">
      <h5 class="offcanvas-title">Create an order</h5>
      <button type="button" class="btn-close" data-bs-dismiss="offcanvas" aria-label="Close"></button>
    </div>
    <div class="offcanvas-body">
      <form id="vendorOrderForm" class="row g-3">
        <h5 class="text-secondary">Vendor Details</h5>
        <div class="col-12">
          <div class="form-floating">
            <input type="text" id="Name" class="form-control" placeholder="Vendor Name" required />
            <label>Vendor Name</label>
          </div>
        </div>
        <div class="col-12">
          <div class="form-floating">
            <textarea id="Location" class="form-control" placeholder="Location" style="height:100px"
              required></textarea>
            <label>Location</label>
          </div>
        </div>
        <div class="col-md-12">
          <div class="form-floating">
            <input type="text" id="Phone" maxlength="10" class="form-control" placeholder="Phone" required />
            <label>Phone</label>
          </div>
        </div>
        <div class="col-md-12">
          <div class="form-floating">
            <input type="email" id="Email" class="form-control" placeholder="Email" />
            <label>Email</label>
          </div>
        </div>
        <hr class="mt-4" />
        <!-- Order Section -->
        <h5 class="text-secondary">Order Details</h5>
        <div class="col-md-12">
          <span id="ProdManagerId" class="form-control d-none">@id</span>
        </div>
        <div class="col-md-12">
          <div class="form-floating">
            <select name="SalesPersonId" class="form-select" id="SalesPersonId">
              <option value="0">- Select a sales person-</option>
              @foreach (DataRow row in dtSalesPersons.Rows)
              {
                <option value="@row["SalesPersonId"]">@row["Name"]</option>
              }
            </select>
            <label>Sales Person ID</label>
          </div>
        </div>
        <div class="col-md-4">
          <div class="form-floating">
            <input type="number" id="Cases025Ltr" class="form-control" value="0" />
            <label>0.25L</label>
          </div>
        </div>
        <div class="col-md-4">
          <div class="form-floating">
            <input type="number" id="Cases05Ltr" class="form-control" value="0" />
            <label>0.5L</label>
          </div>
        </div>
        <div class="col-md-4">
          <div class="form-floating">
            <input type="number" id="Cases1Ltr" class="form-control" value="0" />
            <label>1L</label>
          </div>
        </div>
        <div class="col-md-4">
          <div class="form-floating">
            <input type="number" id="Cases2Ltr" class="form-control" value="0" />
            <label>2L</label>
          </div>
        </div>
        <div class="col-md-4">
          <div class="form-floating">
            <input type="number" id="Cases5Ltr" class="form-control" value="0" />
            <label>5L</label>
          </div>
        </div>
        <div class="col-md-4">
          <div class="form-floating">
            <input type="number" id="Cases20Ltr" class="form-control" value="0" />
            <label>20L</label>
          </div>
        </div>
        <div class="col-md-6">
          <div class="form-floating">
            <input type="number" step="1" id="AmountBilled" class="form-control" value="0" />
            <label>Amount Billed</label>
          </div>
        </div>
        <div class="col-md-6">
          <div class="form-floating">
            <input type="number" step="1" id="AmountPaid" class="form-control" value="0" />
            <label>Amount Paid</label>
          </div>
        </div>
        <div class="col-12 pt-4 d-flex justify-content-end gap-2">
          <button type="reset" class="btn btn-secondary">Reset</button>
          <button type="button" id="btnCreate" class="btn btn-primary">Create</button>
        </div>
        <div class="col-12 pt-4">
          <div class="alert alert-success d-none" id="CreateOrderMsg" role="alert"></div>
        </div>
        <div class="col-12 pt-4">
          <div class="input-group">
            <input type="text" class="form-control" id="clipboard-input" value="url">
            <button class="btn btn-outline-secondary" type="button" data-clipboard-target="#clipboard-input"><i
                class="fa fa-copy me-2"></i>Copy</button>
            <button class="btn btn-outline-primary" type="button"><i class="fa fa-whatsapp me-2"></i></button>
          </div>
        </div>
      </form>
    </div>
  </div>
  <!--[ Jquery Core Js ]-->
  <script src="~/assets/js/theme.js"></script>
  <script src="~/assets/bundles/owlcarousel.bundle.js"></script>
  <script src="~/assets/bundles/dataTables.bundle.js"></script>
  <script src="~/assets/vendor/metismenu/metismenu.js"></script>
